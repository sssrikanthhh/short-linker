generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Url {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  originalUrl String 
  shortCode    String @unique
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
}